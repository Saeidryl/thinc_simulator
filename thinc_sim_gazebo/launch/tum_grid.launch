			<arg name="robot_namespace" value="drone1"/>
			<arg name="x" value="-1.00"/>
			<arg name="y" value="2.00"/>
			<arg name="z" value="0.70"/>
		</include>
	</group>

	<group ns="drone2">
		<param name="tf_prefix" value="drone2"/>
