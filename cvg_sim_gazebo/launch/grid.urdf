<?xml version="1.0"?>


<robot name="grid">
  <link name="base_link">
    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="1.0" />
      <inertia  ixx="1.0" ixy="0.0"  ixz="0.0"  iyy="100.0"  iyz="0.0"  izz="1.0" />
    </inertial>
    <visual>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="1 1 0.1" />
      </geometry>
      <material name="blue">
        <color rgba="0 0 0.8 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="1 1 0.1" />
      </geometry>
    </collision>
  </link>
  <gazebo>
    <static>true</static>
  </gazebo>


  <link name="link1">
    <inertial>
      <origin xyz="1 0 0"/>
      <mass value="1.0" />
      <inertia  ixx="1.0" ixy="0.0"  ixz="0.0"  iyy="100.0"  iyz="0.0"  izz="1.0" />
    </inertial>
    <visual>
      <origin xyz="1 0 0"/>
      <geometry>
        <box size="1 1 0.1" />
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="1 0 0"/>
      <geometry>
        <box size="1 1 0.1" />
      </geometry>
    </collision>
  </link>
  <gazebo>
    <static>true</static>
  </gazebo>


  <joint name="base_to_link1" type="fixed">
    <parent link="base_link"/>
    <child link="link1"/>
    <origin xyz="0 0 0"/>
  </joint>
</robot>